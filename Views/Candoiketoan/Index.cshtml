<script>
    $(document).ready(function () {
        ["dk_no", "dk_co", "ps_no", "ps_co"].forEach(sumAll);
    });
    function sumAll(type) {

        // ===== 1. cộng con → cha =====
        $("#tb4 input[id$='_" + type + "']").each(function () {

            const match = this.id.match(/cell_(\d+)_/);
            if (!match) return;

            const acc = match[1];
            const parent = acc.slice(0, -1);

            if ($("#cell_" + parent + "_" + type).length) {
                sumChildrenToParent(parent, type);
            }
        });

        // ===== 2. tổng toàn bảng =====
        sumTotal(type);
    }
    function sumChildrenToParent(parent, type) {

        let sum = 0;

        $("input[id^='cell_" + parent + "'][id$='_" + type + "']").each(function () {

            const acc = this.id.match(/cell_(\d+)_/)[1];

            if (acc.length === parent.length + 1) {
                sum += parseNumber(this.value);
            }
        });

        const parentInput = $("#cell_" + parent + "_" + type)[0];
        parentInput.value = sum;

        formatNumber(parentInput);
    }
    function updateParent(childInput) {

        const match = childInput.id.match(/cell_(\d+)_(.+)/);
        if (!match) return;

        const acc = match[1];     // 13811
        const type = match[2];    // dk_no | dk_co | ps_no...

        const parent = acc.slice(0, -1);

        if ($("#cell_" + parent + "_" + type).length) {

            sumChildrenToParent(parent, type);

            // đệ quy cập nhật tiếp cha cấp trên
            updateParent(document.getElementById("cell_" + parent + "_" + type));
        }

        // cập nhật dòng tổng
        sumTotal(type);
    }
    function parseNumber(val) {
        return parseFloat(val.replace(/\./g, '').replace(/,/g, '.')) || 0;
    }
    function sumTotal(type) {

        let total = 0;

        $("#tb4 input[id$='_" + type + "']").each(function () {

            const match = this.id.match(/cell_(\d+)_/);
            if (!match) return;   // ⭐ tránh lỗi

            const acc = match[1];

            if (acc.length === 3) {
                total += parseNumber(this.value);
            }
        });

        const totalInput = document.getElementById("cell_total_" + type);
        totalInput.value = total;

        formatNumber(totalInput);
    }
    $("#tb4").on("blur", "input", function () {
        if ($(this).val().trim() === "") {
            $(this).val(0);
        }
    });
    $("#tb4").on("input", "input", function () {

        if (!this.id.startsWith("cell_")) return;

        updateParent(this);

    });
    function opentab(tabNumber) {
        if (tabNumber == 1) {
            $("#titleName").html("BÁO CÁO TÌNH HÌNH TÀI CHÍNH")
        }
        if (tabNumber == 2) {
            $("#titleName").html("BÁO CÁO KẾT QUẢ HOẠT ĐỘNG KINH DOANH")
        }
        if (tabNumber == 3) {
            $("#titleName").html("BÁO CÁO LƯU CHUYỂN TIỀN TỆ")
        }
        if (tabNumber == 4) {
            $("#titleName").html("BẢNG CÂN ĐỐI TÀI KHOẢN")
        }
        // Ẩn tất cả tab content
        for (let i = 1; i <= 4; i++) {
            $("#tb" + i).hide();
        }

        // Hiển thị tab được chọn
        $("#tb" + tabNumber).show();

        // Xóa class active từ tất cả tab links
        for (let i = 1; i <= 4; i++) {
            $("#acl" + i).removeClass("actv");
        }

        // Thêm class active cho tab link được chọn
        $("#acl" + tabNumber).addClass("actv");
    }
    function cleanVNNumber(str) {
        if (!str) return 0;
        str = str.replace(/\./g, '');          // xóa dấu chấm nghìn
        str = str.replace(/,/g, '.');          // thay dấu phẩy thập phân thành .
        return Number(str) || 0;
    }
    function escapePath(path) {
        return path.replace(/\\/g, "\\\\");
    }
    function cleanToNumber(str) {
        if (!str || typeof str !== 'string') return 0;
        // Xóa hết dấu chấm (phân cách nghìn), dấu phẩy nếu có, khoảng trắng
        var cleaned = str.replace(/[\.,\s]/g, '');
        var num = Number(cleaned);
        return isNaN(num) ? 0 : num;  // fallback về 0 nếu không parse được
    }
    function toggle300() {
        // Hàm helper: chuyển chuỗi kiểu VN ("1.000.000") thành số nguyên
      

        // Lấy giá trị và clean
        var val100Cuoi = $("#cell_100_cuoi").val();
        var val200Cuoi = $("#cell_200_cuoi").val();
        var val100Dau = $("#cell_100_dau").val();
        var val200Dau = $("#cell_200_dau").val();

        // Tính tổng
        var result1 = cleanToNumber(val100Cuoi) + cleanToNumber(val200Cuoi);
        var result2 = cleanToNumber(val100Dau) + cleanToNumber(val200Dau);

        // Ghi log để debug (xem giá trị gốc và sau khi clean)
        console.log("cell_100_cuoi gốc:", val100Cuoi, "→ sau clean:", cleanToNumber(val100Cuoi));
        console.log("cell_200_cuoi gốc:", val200Cuoi, "→ sau clean:", cleanToNumber(val200Cuoi));
        console.log("Tổng cuối năm:", result1);

        // Gán giá trị (không format lại ở đây, để hàm khác xử lý nếu cần)
        $("#cell_300_cuoi").val(result1);
        $("#cell_300_dau").val(result2);
