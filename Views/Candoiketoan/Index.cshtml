<script>
    function opentab(tabNumber) {
        if (tabNumber == 1) {
            $("#titleName").html("BÁO CÁO TÌNH HÌNH TÀI CHÍNH")
        }
        if (tabNumber == 2) {
            $("#titleName").html("BÁO CÁO KẾT QUẢ HOẠT ĐỘNG KINH DOANH")
        }
        if (tabNumber == 3) {
            $("#titleName").html("BÁO CÁO LƯU CHUYỂN TIỀN TỆ")
        }
        if (tabNumber == 4) {
            $("#titleName").html("BẢNG CÂN ĐỐI TÀI KHOẢN")
        }
        // Ẩn tất cả tab content
        for (let i = 1; i <= 4; i++) {
            $("#tb" + i).hide();
        }

        // Hiển thị tab được chọn
        $("#tb" + tabNumber).show();

        // Xóa class active từ tất cả tab links
        for (let i = 1; i <= 4; i++) {
            $("#acl" + i).removeClass("actv");
        }

        // Thêm class active cho tab link được chọn
        $("#acl" + tabNumber).addClass("actv");
    }

    // Có thể gọi trực tiếp:
    // openTab(1); // Mở tab 1
    // openTab(2); // Mở tab 2
    // openTab(3); // Mở tab 3
    // openTab(4); // Mở tab 4
   
   function exportXML() {
    const baseUrl = '@Url.Action("CreateTaxXMLFull", "Candoiketoan")';
       const url = `${baseUrl}?nam=${'@ViewBag.NamTC'}`;
    window.open(url);
}
    function gui() {
        $('#loadingModal').modal('show');

        var stt = 1;


        var datagui = {
            path: $("#path").val(),
            nlbieu: $("#nlbieu").val(),
            giamdoc: $("#giamdoc").val(),
            ketoantruong: $("#ketoantruong").val(),
            chungchi: $("#chungchi").val(),
            dvcc: $("#dvcc").val(),
            ngayky: $("#ngaylap").val(),
            nam:'@ViewBag.NamTC'
        }
        var dataArray = [];

        $.ajax({
            url: "/Candoiketoan/CreateData",
            method: "POST",
            data: datagui,
            success: function (res) {
                alert(res);
                if (res >= 0) {
                    $('tr').each(function () {
                        var maso = 0;
                        var DauNam = 0;
                        var CuoiKy = 0;

